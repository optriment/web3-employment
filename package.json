{
  "name": "web3-employment",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --no-cache && npx eslint . --ext .ts,.tsx,.js,.jsx && npx prettier --check .",
    "lint:prisma": "npx prisma validate",
    "lint-fix": "next lint --fix",
    "analyze": "cross-env ANALYZE=true npm run-script build",
    "test:db:prepare": "dotenv -f .env.test -- npx prisma migrate reset --force --skip-seed",
    "test": "dotenv -f .env.test -- npx prisma migrate deploy && jest -i",
    "prisma:generate_client": "prisma generate",
    "db:migrate": "prisma migrate deploy"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.1.0",
    "@next-auth/prisma-adapter": "^1.0.6",
    "@next/bundle-analyzer": "13.3.4",
    "@prisma/client": "^4.14.1",
    "@sentry/nextjs": "^7.50.0",
    "@tronweb3/tronwallet-adapter-react-hooks": "^1.1.2",
    "@tronweb3/tronwallet-adapter-react-ui": "^1.1.3",
    "@tronweb3/tronwallet-adapters": "^1.1.5",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.4",
    "cross-env": "^7.0.3",
    "mobile-detect": "^1.4.5",
    "next": "13.4.2",
    "next-auth": "^4.22.1",
    "nodemailer": "^6.9.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.0",
    "semantic-ui-css": "^2.5.0",
    "semantic-ui-react": "^2.1.4",
    "tronweb": "^5.1.0",
    "uuid": "^8.3.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "13.3.1",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.3",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.40.0",
    "eslint-config-next": "13.4.2",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-autofix": "^1.1.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest-extended": "^2.0.0",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "node-mocks-http": "^1.11.0",
    "prettier": "^2.8.4",
    "prisma": "^4.14.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
